{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yixinlyudf1"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"GitHubforScottLyu_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHubforScottLyu'"
		},
		"RestService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestService1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://yixinlyuds598.dfs.core.windows.net/"
		},
		"GitHubforScottLyu_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Scottlyu134"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org/data/2.5/air_pollution/history?lat=42.3601&lon=-71.0589&start=1695081600&end=1726617600&appid=9cbcdd7a500b50c08496a2255abfe4fe"
		},
		"RestService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Scottlyu134"
		},
		"weatherKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://OpenwheatherAPI.vault.azure.net/"
		},
		"weatherboston1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/data/2.5/history/city"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "yixinlyudf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"GitHubforScottLyu_credential": {
						"value": "[parameters('GitHubforScottLyu_credential')]"
					},
					"RestService1_password": {
						"value": "[parameters('RestService1_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"GitHubforScottLyu_properties_typeProperties_username": {
						"value": "[parameters('GitHubforScottLyu_properties_typeProperties_username')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_userName": {
						"value": "[parameters('RestService1_properties_typeProperties_userName')]"
					},
					"weatherKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('weatherKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"weatherboston1_properties_typeProperties_url": {
						"value": "[parameters('weatherboston1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}